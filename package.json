{
  "name": "@mekefly/worm-ui",
  "version": "0.0.1",
  "main": "dist/worm-ui.umd.js",
  "typings": "src/index.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "dev": "pnpm docs:dev",
    "docs:dev": "vite docs --config docs/vite.config.ts",
    "docs:build": "vue-tsc --noEmit && vite build",
    "docs:preview": "vite preview",
    "build": "vue-tsc --noEmit && pnpm vite build",
    "build:cjs": "tsc -b --force tsconfig.cjs.json",
    "build:esm": "tsc -b --force tsconfig.esm.json"
  },
  "dependencies": {
    "@wormery/utils": "^1.0.1",
    "@wormery/wtsc": "2.0.0-bate.27",
    "animejs": "^3.2.1",
    "marked": "^4.0.12",
    "vue": "^3.2.25",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/animejs": "^3.1.4",
    "@types/marked": "^4.0.3",
    "@types/node": "^17.0.8",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "ionicons": "^6.0.1",
    "less": "^4.1.2",
    "mavon-editor": "^3.0.0-beta",
    "sass": "^1.45.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-md2vue": "^1.1.1",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.29.8"
  }
}
